{
  "extends": ["../../.eslintrc.json"],
  "env": {
    "jest": true
  },
  "rules": {
    "no-case-declarations": "warn",
    "@typescript-eslint/interface-name-prefix": "warn",
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "camelcase": [
      "warn",
      {
        "properties": "never"
      }
    ],
    "@typescript-eslint/camelcase": [
      "warn",
      {
        "properties": "never"
      }
    ]
  }
}
