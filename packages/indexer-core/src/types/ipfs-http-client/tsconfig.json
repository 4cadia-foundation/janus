{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2015",
      "es2016",
      "es2016",
      "dom",
      "dom.iterable"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "../",
    "typeRoots": [
      "../"
    ],
    "types": [
      "node"
    ],
    "paths": {
      "ipfs-http-client": [
        "."
      ]
    },
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "*.ts"
  ]
}
